{
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": ".",
  "functions": {
    "api/send-email.cjs": {
      "runtime": "@vercel/node@3.1.0"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}